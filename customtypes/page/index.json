{
  "format": "page",
  "id": "page",
  "label": "Page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "type": {
        "type": "Select",
        "config": {
          "label": "Type",
          "placeholder": "",
          "options": [
            "Live reports",
            "Discoveries",
            "Press Releases",
            "Features",
            "Galleries",
            "Other"
          ],
          "default_value": "Live reports"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "interview": {
              "type": "SharedSlice"
            },
            "separator": {
              "type": "SharedSlice"
            },
            "contact_form": {
              "type": "SharedSlice"
            },
            "image_grid": {
              "type": "SharedSlice"
            },
            "image_list": {
              "type": "SharedSlice"
            },
            "setlist": {
              "type": "SharedSlice"
            },
            "taglist": {
              "type": "SharedSlice"
            },
            "social_links": {
              "type": "SharedSlice"
            },
            "purchase": {
              "type": "SharedSlice"
            },
            "authors": {
              "type": "SharedSlice"
            },
            "video": {
              "type": "SharedSlice"
            },
            "quote": {
              "type": "SharedSlice"
            },
            "doc_list": {
              "type": "SharedSlice"
            },
            "single_button": {
              "type": "SharedSlice"
            },
            "rich_text": {
              "type": "SharedSlice"
            },
            "carousel": {
              "type": "SharedSlice"
            },
            "image": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
